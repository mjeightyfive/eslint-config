{
    "name": "@mjeightyfive/eslint-config",
    "version": "0.0.53",
    "description": "eslint config",
    "main": "index.js",
    "scripts": {
        "preversion": "npm t",
        "version": "auto-changelog -p --commit-limit false --tag-prefix '' && git add CHANGELOG.md",
        "postversion": "git push && git push --tags",
        "lint": "eslint . --config=.eslintrc.js",
        "test": "npm run lint && jest ./tests/**.js"
    },
    "devDependencies": {
        "@babel/preset-env": "7.14.2",
        "@typescript-eslint/eslint-plugin": "4.23.0",
        "@typescript-eslint/parser": "4.23.0",
        "auto-changelog": "2.2.1",
        "babel-eslint": "10.1.0",
        "eslint": "7.12.1",
        "eslint-config-airbnb": "18.2.1",
        "eslint-config-prettier": "8.3.0",
        "eslint-config-react-app": "6.0.0",
        "eslint-plugin-flowtype": "5.7.2",
        "eslint-plugin-import": "2.23.1",
        "eslint-plugin-jest": "24.3.6",
        "eslint-plugin-json": "3.0.0",
        "eslint-plugin-jsx-a11y": "6.4.1",
        "eslint-plugin-node": "11.1.0",
        "eslint-plugin-prettier": "3.4.0",
        "eslint-plugin-promise": "5.1.0",
        "eslint-plugin-react": "7.23.2",
        "eslint-plugin-react-hooks": "4.2.0",
        "jest": "26.6.3",
        "prettier": "2.3.0",
        "react": "17.0.2",
        "tsutils": "3.21.0",
        "typescript": "4.2.4"
    },
    "peerDependencies": {
        "eslint": "^7.12.1"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/mjeightyfive/eslint-config.git"
    },
    "keywords": [
        "lint",
        "eslint"
    ],
    "author": "Marcin <mjeightyfive@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/mjeightyfive/eslint-config/issues"
    },
    "homepage": "https://github.com/mjeightyfive/eslint-config#readme"
}
