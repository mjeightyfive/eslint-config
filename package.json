{
    "name": "@mjeightyfive/eslint-config",
    "version": "0.0.49",
    "description": "eslint config",
    "main": "index.js",
    "scripts": {
        "preversion": "npm t",
        "version": "auto-changelog -p --commit-limit false --tag-prefix '' && git add CHANGELOG.md",
        "postversion": "git push && git push --tags",
        "lint": "eslint . --config=.eslintrc.js",
        "test": "npm run lint && jest ./tests/**.js"
    },
    "dependencies": {},
    "devDependencies": {
        "@babel/preset-env": "7.9.5",
        "@typescript-eslint/eslint-plugin": "2.28.0",
